{
    "name": "Activity",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^7",
        "@sap/cds-hana": "^2",
        "@sap/xssec": "^3",
        "express": "^4",
        "npx": "^10.2.2",
        "passport": "^0",
        "rimraf": "^5.0.5"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1",
        "@sap/cds-dk": "^7",
        "@sap/ux-specification": "^1.120.0",
        "rimraf": "^3.0.2"
    },
    "scripts": {
        "start": "cds-serve",
        "watch-project1": "cds watch --open project1/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy Activity --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
    },
    "cds": {
        "requires": {
            "db": "hana",
            "[production]": {
                "auth": {
                    "kind": "xsuaa"
                }
            },
            "auth": {
                "kind": "mocked",
                "users": {
                    "admin": {
                        "password": "123456"
                    }
                }
            }
        }
    },
    "sapux": [
        "app/project1"
    ]
}